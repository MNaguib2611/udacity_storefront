{
  "name": "udacity_storefront",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "engines": {
    "node": "16.15.0"
  },
  "scripts": {
    "start": "node server.js",
    "build": "rm -rf www && npx tsc && cp -rf dist www && cp -R .elasticbeanstalk www/.elasticbeanstalk && cp database.json www/database.json&& cp package.json www/package.json && cd www && zip -r Archive.zip . && cd ..",
    "jasmine": "jasmine",
    "test": "ENV=test db-migrate -m ./src/migrations down --env test -c 5 && ENV=test db-migrate -m ./src/migrations --env test up && tsc && ENV=test  yarn run jasmine",
    "lint": "eslint --cache --ignore-path .gitignore --ext .js,.ts ./",
    "prettier": "prettier .",
    "format:verify": "yarn run prettier  --check .",
    "format": "yarn run prettier --write",
    "verify": "yarn run lint && yarn run format",
    "deploy": "yarn run build && eb use store-front-api-dev && eb deploy"
  },
  "keywords": [
    "starter-code-typescrript-express"
  ],
  "author": "Mohammed Naguib",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jasmine": "^4.3.1",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^18.11.11",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "supertest": "^6.3.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.8.0",
    "typescript": "^4.9.4"
  }
}
